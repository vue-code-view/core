  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@microsoft/api-extractor": "^7.34.4",
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vue": "^3.2.47",
